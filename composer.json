    "version": "dev-master",
    "keywords": [
        "framework",
        "zf2",
        "search engine",
        "lucene",
        "full text",
        "pdf",
        "doc",
        "xls",
        "mp3",
        "jpg"
    ],
    "homepage": "https://ifile.isapp.it",
    "authors": [
        {
        	"name": "Giampaolo Losito",
           "email": "giampaolo@isapp.it",
            "homepage": "http://www.isapp.it",
